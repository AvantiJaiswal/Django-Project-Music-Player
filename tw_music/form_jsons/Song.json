            "options": {
                "A": "A1",
                "B": "B1"
            },
            "required": "true"
        },
        {
            "tag": "input",
            "type": "checkbox",
            "class": "form-control",
            "label": "Favourite",
            "id": "favourite",
            "name": "favourite",
            "label1":{
                "A1":"A1"
            }
        },
        {
            "tag": "input",
            "type": "text",
            "class": "form-control",
            "label": "Album name",
            "id": "album_name",
            "name": "album_name",
            "maxlength": "100",
            "required": "true",
            "placeholder": "Enter album name"
        },
        {
            "tag": "input",
            "type": "text",
            "class": "form-control",
